package practice;

class practiceJava {
    public static void main(String[] args) {
    	int arr[]= {1,2,3};
    	helper(arr,0,"");
        
    }

	private static void helper(int[] arr, int i, String ans) {
		// TODO Auto-generated method stub
		if(i==arr.length) {
			System.out.println(ans);
			return;
		}
		
		helper(arr,i+1,ans+arr[i]);
		helper(arr,i+1,ans);
				
		
	}

   
}
