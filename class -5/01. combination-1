package practice;
import java.util.*;
import java.util.Arrays;
import java.math.BigInteger;
class practiceJava {
    public static void main(String[] args) {
    	String arr[]={"kurkure","Pepsi","mangobyte"};
    
    	 helper(arr," ",0);

    	 
    }

	private static void helper(String[] arr, String ans,int count) {
		// TODO Auto-generated method stub
		if(count==3) {
			System.out.println(ans);
			return;
		}
	
		helper(arr,ans+""+arr[0],count+1);
		helper(arr,ans+""+arr[1],count+1);
		helper(arr,ans+""+arr[2],count+1);
		
	}
    
    

 
    }
       
        	
    

